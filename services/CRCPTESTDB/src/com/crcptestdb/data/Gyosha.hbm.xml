<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.crcptestdb.data.Gyosha" table="GYOSHA" dynamic-insert="false" dynamic-update="false">
        <id name="gyoshaCd" type="string">
            <column name="GYOSHA_CD" length="9"/>
            <generator class="assigned"/>
        </id>
        <property name="address" type="string">
            <column name="ADDRESS" length="384" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>